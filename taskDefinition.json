{
  "family": "code-compiler-tecathon-containers",
  "containerDefinitions": [
    {
      "name": "code-compiler-tecathon",
      "image": "551961123650.dkr.ecr.region.amazonaws.com/code-compiler:latest",
      "cpu": 10,
      "memory": 300,
      "portMappings": [
          {
              "containerPort": 80,
              "hostPort": 3000
          }
      ],
      "essential": true,
      "environment": [
          {
              "name": "NODE_ENV",
              "value": "production"
          }
      ]
    }
  ]
}